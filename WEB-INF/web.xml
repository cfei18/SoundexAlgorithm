<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  
  <display-name>Soundex</display-name>
  <description>Soundex Servlet by cfei</description>
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
  <servlet-name>SoundexServlet</servlet-name>
  <servlet-class>cfei.soundex.SoundexServlet</servlet-class>
  <description>The simple servlet to provide the Soundex algorithm</description>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>SoundexServlet</servlet-name>
  <url-pattern>/soundex</url-pattern>
  </servlet-mapping>
  
</web-app>